{
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "debug": true,
          "corejs": { "version": 3, "proposals": true }
        //   "corejs": 3, // es2020 で変換した場合
        }
      ],
      "@babel/typescript"
    ],
    // esnext で変換した場合
    // "plugins": ["@babel/plugin-proposal-class-properties"]
    // "@babel/typescript" を使った場合
    "plugins": ["@babel/proposal-class-properties", "@babel/proposal-object-rest-spread"]
}
  